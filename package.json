    "multer-s3-transform": "^2.3.2",
    "next": "^9.1.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
